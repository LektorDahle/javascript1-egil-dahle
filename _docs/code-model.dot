digraph RanyDays {
    compound=true;
    api [ label = "API-connection", shape= "cylinder" ];
    localStorage [ label = "LocalStorage", shape = "folder" ];

    
    subgraph main{
        cluster=true;
        label = "main.js";
        style=dotted;
        render [ label = "render" ];
        DOMContentLoaded [ label = "DOMContentLoaded", shape = "rectangle" ];
    }
    subgraph headerFooter{
        cluster=true;
        label = "header-footer.js";
        style=dotted;
        subgraph Header{
            label = "Header";
            style=filled
            addLogo [ label = "addLogo" ];
        }
        subgraph Footer{
            label = "Footer";
            style=filled
         dd[ label = "dd"];
        }
    }

    subgraph getData{
        cluster=true;
        label = "getData.js";
        style=dotted;
        subgraph GetData{
            label = "GetData";
            style=filled
            getAll[ label = "getAll"];
            getOne[ label = "getOne"];
            getSimilar[ label = "getSimilar"];
        }
    }

    subgraph products{
        cluster=true;
        label = "products.js";
        style=dotted;
        showAllProducts;
        showOneProduct
        showSimilarProduct
    }

    addLogo -> render [ltail=Header];
    dd -> render [ltail=Footer];
    getOne -> api [ltail=GetData];
    getOne -> localStorage;
    getAll -> localStorage;
    getSimilar -> localStorage;
    render -> DOMContentLoaded;
}